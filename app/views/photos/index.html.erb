<p><%= notice %></p>
<h1>Ujistagram</h1>

<% @photos.each do |photo| %>
  <p>タイトル:<%= photo.title %></p>
  <p>本文:<%= photo.content %></p>
  <%=link_to"edit",edit_photo_path(photo.id)%>
  <%=link_to"delete",photo_path(photo.id),method: :delete,data:{confirm:"本当に編集していいですか？"}%>
<% end %>

<%=link_to"Photos",new_photo_path %>
